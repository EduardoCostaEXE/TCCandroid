<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="#000000"
    android:foregroundTint="#000000"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="169px"
        android:layout_height="169px"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="82dp"
        tools:srcCompat="@drawable/ImgUserDefaultImg" />

    <Button
        android:id="@+id/button"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/ImgArrowBack"
        android:backgroundTint="#000000"
        android:backgroundTintMode="add"
        android:drawableTint="#000000"
        android:drawableTintMode="add"
        android:foregroundTint="#000000"
        android:shadowColor="#FFFFFF"
        android:textColorHighlight="#000000"
        android:textColorLink="#000000"
        app:iconTint="#000000"
        app:rippleColor="#000000"
        app:strokeColor="#000000"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/textUsername"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Username"
        android:textColor="#000000"
        android:textSize="25sp"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="160dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Atende a: Defina o local"
        android:textColor="#000000"
        android:textSize="15.3sp"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="205dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Instituições de ensino atendidas:"
        android:textColor="#000000"
        android:textSize="15sp"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="266dp" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="356dp"
        android:layout_height="205dp"
        android:background="#E5E5E5"
        android:backgroundTint="#E5E5E5"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="306dp">

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Endereço:"
            android:textColor="#000000"
            android:textSize="13sp"
            tools:layout_editor_absoluteX="15dp"
            tools:layout_editor_absoluteY="29dp" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Manhã: Não atende"
            android:textColor="#000000"
            android:textSize="13sp"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="135dp" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Noite: Não atende"
            android:textColor="#000000"
            android:textSize="13sp"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="153dp" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Horário: Saída às 00:00 e retorno às 00:00"
            android:textColor="#000000"
            android:textSize="13sp"
            tools:layout_editor_absoluteX="15dp"
            tools:layout_editor_absoluteY="171dp" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Universidade indefinida:"
            android:textColor="#000000"
            android:textSize="14sp"
            tools:layout_editor_absoluteX="15dp"
            tools:layout_editor_absoluteY="10dp" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Mensalidade: R$"
            android:textColor="#000000"
            android:textSize="14sp"
            tools:layout_editor_absoluteX="15dp"
            tools:layout_editor_absoluteY="66dp" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Itinerário: "
            android:textColor="#000000"
            android:textSize="14sp"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="116dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="77px"
        android:layout_height="100px"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="255dp"
        tools:srcCompat="@drawable/ImgEducationIcon" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Informações do veículo:"
        android:textColor="#000000"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="532dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Galeria de fotos:"
        android:textColor="#000000"
        android:textSize="13sp"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="583dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Placa:"
        android:textColor="#000000"
        android:textSize="13sp"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="551dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="300dp"
        android:layout_height="200dp"
        android:background="#C4C4C4"
        android:backgroundTint="#C4C4C4"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="617dp"
        tools:srcCompat="@drawable/ImgGallery" />

</androidx.constraintlayout.widget.ConstraintLayout>